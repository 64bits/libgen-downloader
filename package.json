{
  "name": "libgen-downloader",
  "version": "1.3.0",
  "description": "a simple tool to search and download pdfs via command line interface from libgen",
  "main": "./bin/index.js",
  "bin": {
    "libgen-downloader": "./bin/index.js",
    "obsfx@libgen-downloader": "./bin/index.js"
  },
  "scripts": {
    "standalone": "pkg . --out-path ./standalone-executables",
    "run": "ts-node ./src/index.ts",
    "inspect": "node --inspect -r ts-node/register ./src/index.ts",
    "build": "tsc -b",
    "watch": "tsc -b --watch"
  },
  "keywords": [
    "libgen",
    "downloader",
    "tool",
    "cli",
    "parser",
    "output",
    "user-friendly",
    "command",
    "line",
    "library",
    "genesis"
  ],
  "author": "obsfx",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/obsfx/libgen-downloader.git"
  },
  "devDependencies": {
    "@types/content-disposition": "^0.5.3",
    "@types/jsdom": "^12.2.4",
    "@types/minimist": "^1.2.0",
    "@types/node": "^12.12.38",
    "@types/node-fetch": "^2.5.4",
    "@types/uuid": "^8.0.0",
    "pkg": "^4.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "content-disposition": "^0.5.3",
    "jsdom": "^16.2.0",
    "minimist": "^1.2.5",
    "node-fetch": "^2.6.0",
    "uuid": "^8.2.0"
  }
}
