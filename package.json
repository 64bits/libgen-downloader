{
  "name": "libgen-downloader",
  "version": "1.3.8",
  "description": "a simple tool to search and download pdfs via terminal line interface from libgen",
  "main": "./bin/index.js",
  "bin": {
    "libgen-downloader": "./bin/index.js",
    "obsfx@libgen-downloader": "./bin/index.js"
  },
  "homepage": "https://github.com/obsfx/libgen-downloader",
  "scripts": {
    "run": "DEV=true ts-node ./src/index.ts",
    "run-dt": "npx react-devtools",
    "inspect": "node --inspect-brk -r ts-node/register ./src/index.ts",
    "build": "tsc -b",
    "watch": "tsc -b --watch",
    "prepare": "husky install"
  },
  "keywords": [
    "libgen",
    "downloader",
    "tool",
    "cli",
    "parser",
    "output",
    "user-friendly",
    "command",
    "line",
    "library",
    "genesis"
  ],
  "author": "obsfx<balandiomer@gmail.com>",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/obsfx/libgen-downloader.git"
  },
  "devDependencies": {
    "@types/bent": "7.3.2",
    "@types/jsdom": "16.2.14",
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "husky": "7.0.4",
    "prettier": "2.5.1",
    "react-devtools": "4.24.0",
    "ts-node": "10.7.0",
    "typescript": "4.6.2"
  },
  "dependencies": {
    "bent": "7.3.12",
    "figures": "4.0.0",
    "ink": "3.2.0",
    "ink-spinner": "4.0.3",
    "ink-text-input": "4.0.3",
    "jsdom": "19.0.0",
    "react": "17.0.2"
  }
}
