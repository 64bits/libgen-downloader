{
  "name": "libgen-downloader",
  "version": "0.1.18",
  "description": "a simple tool to search and download pdfs via command line interface from libgen",
  "main": "./build/index.js",
  "bin": {
    "libgen-downloader": "./bin/index.js",
    "obsfx@libgen-downloader": "./bin/index.js"
  },
  "scripts": {
    "run": "node ./dist/index.js",
    "build": "tsc -b",
    "minify": "minify ./dist -d ./build",
    "watch": "tsc -b --watch",
    "link": "npm link --force",
    "rmout": "rm -R ./dist"
  },
  "keywords": [
    "libgen",
    "downloader",
    "tool",
    "cli",
    "parser",
    "output",
    "user-friendly"
  ],
  "author": "obsfx",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/obsfx/libgen-cli-downloader.git"
  },
  "devDependencies": {
    "@types/cli-spinner": "^0.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/jsdom": "^12.2.4",
    "@types/node": "^12.12.21",
    "@types/node-fetch": "^2.5.4",
    "@types/progress": "^2.0.3",
    "babel-minify": "^0.5.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "cli-spinner": "^0.2.10",
    "inquirer": "^7.0.1",
    "jsdom": "^15.2.1",
    "kleur": "^3.0.3",
    "node-fetch": "^2.6.0",
    "progress": "^2.0.3"
  }
}
