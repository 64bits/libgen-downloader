{
  "name": "libgen-downloader",
  "version": "1.2.5",
  "description": "a simple tool to search and download pdfs via command line interface from libgen",
  "main": "./bin/index.js",
  "bin": {
    "libgen-downloader": "./bin/index.js",
    "obsfx@libgen-downloader": "./bin/index.js"
  },
  "scripts": {
    "run": "DEV=true ts-node ./src/index.tsx",
    "run-dt": "npx react-devtools",
    "inspect": "node --inspect -r ts-node/register ./src/index.tsx",
    "build": "tsc -b",
    "watch": "tsc -b --watch",
    "standalone": "pkg . --out-path ./standalone-executables"
  },
  "keywords": [
    "libgen",
    "downloader",
    "tool",
    "cli",
    "parser",
    "output",
    "user-friendly",
    "command",
    "line",
    "library",
    "genesis"
  ],
  "author": "obsfx",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/obsfx/libgen-downloader.git"
  },
  "devDependencies": {
    "@types/content-disposition": "^0.5.3",
    "@types/jsdom": "^12.2.4",
    "@types/minimist": "^1.2.1",
    "@types/node": "^12.19.4",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^16.9.56",
    "@types/uuid": "^8.3.0",
    "pkg": "^4.4.9",
    "react-devtools": "^4.10.0",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "content-disposition": "^0.5.3",
    "figures": "^3.2.0",
    "ink": "^3.0.8",
    "ink-spinner": "^4.0.1",
    "ink-text-input": "^4.0.1",
    "ink-use-stdout-dimensions": "^1.0.5",
    "jsdom": "^16.4.0",
    "minimist": "^1.2.5",
    "node-fetch": "^2.6.0",
    "prettysize": "^2.0.0",
    "react": "^17.0.1",
    "uuid": "^8.3.1",
    "zustand": "^3.1.4"
  }
}
